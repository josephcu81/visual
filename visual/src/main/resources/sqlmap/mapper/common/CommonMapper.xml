<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.apfs.common.mapper.CommonMapper">
	<select id="selectSitoCode" parameterType="java.util.Map">
		SELECT 
	        cd AS code
	        ,cd_nm AS name
	        ,etc1 AS lable
	    FROM comm_cd
	    WHERE grp_cd = '300'
	</select>
	<select id="selectKmaAreaCode" parameterType="java.util.Map">
		SELECT 
	        area_id AS code
	        ,area_nm AS name
	    FROM kma_area
	</select>
	
</mapper>